	require_once("../clase/encriptar.class.php");
	$crypto=new encriptar();
	
	echo $crypto->codificar("mysql")."<br>";//Motor
        echo $crypto->codificar("localhost")."<br>";//Servidor
        echo $crypto->codificar("frame_precenso")."<br>";//DB;
        echo $crypto->codificar("frame_precenso")."<br>";//Usuario
        echo $crypto->codificar("admin_precenso2013")."<br>";//Clave
        echo $crypto->codificar("precenso_")."<br>";//Prefijo
        
	
